{
  "name": "crm7",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": "22.x",
    "pnpm": "10.7.1"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "NODE_NO_WARNINGS=1 NODE_OPTIONS='--no-warnings' next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "types": "supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > lib/types/database.ts",
    "types:watch": "nodemon --watch supabase/migrations -e sql -x 'pnpm types'",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "types:sync": "supabase gen types typescript --local > lib/database.types.ts",
    "preinstall": "npx only-allow pnpm",
    "check-pm": "./scripts/check-package-manager.sh",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "validate": "pnpm lint && pnpm test && pnpm build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.6.0",
    "@hookform/resolvers": "3.10.0",
    "@prisma/client": "5.22.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-hover-card": "^1.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.3",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@supabase/supabase-js": "2.49.4",
    "@tanstack/react-table": "^8.0.0",
    "@sentry/nextjs": "8.55.0",
    "@sentry/types": "8.55.0",
    "@supabase/ssr": "0.5.2",
    "@tanstack/react-query": "5.66.9",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.0.4",
    "cssnano": "7.0.6",
    "d3": "^7.8.5",
    "date-fns": "3.3.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "exceljs": "4.4.0",
    "framer-motion": "^11.0.0",
    "globals": "^13.24.0",
    "ioredis": "5.5.0",
    "lucide-react": "0.474.0",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.11",
    "next-themes": "0.4.4",
    "nodemailer": "6.10.0",
    "puppeteer": "24.2.0",
    "react": "19.0.0",
    "react-chartjs-2": "5.3.0",
    "react-day-picker": "9.5.1",
    "react-dom": "19.0.0",
    "react-dropzone": "14.3.5",
    "react-error-boundary": "4.0.12",
    "react-hook-form": "7.54.2",
    "react-hot-toast": "2.5.1",
    "react-hotkeys-hook": "4.6.1",
    "react-pdf": "9.2.1",
    "recharts": "2.15.1",
    "redis": "4.7.0",
    "tailwind-merge": "3.0.1",
    "tailwindcss-animate": "1.0.7",
    "ts-morph": "^21.0.1",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@next/bundle-analyzer": "15.1.6",
    "@next/eslint-plugin-next": "15.1.6",
    "@types/classnames": "2.3.4",
    "@types/d3": "7.4.3",
    "@types/glob": "8.1.0",
    "@types/node": "22.13.1",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "10.4.17",
    "chalk": "5.4.1",
    "commander": "13.1.0",
    "eslint": "8.56.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "11.0.1",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "ioredis-mock": "8.9.0",
    "lint-staged": "15.2.2",
    "node-mocks-http": "1.16.2",
    "ora": "8.2.0",
    "postcss": "8.4.35",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.4.17",
    "typescript": "5.3.3",
    "typescript-eslint": "8.24.0",
    "vite": "5.4.14",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "^1.2.2",
    "winston": "3.17.0",
    "zod": "3.24.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript"
      ]
    },
    "overrides": {
      "@opentelemetry/api": "1.9.0",
      "@types/node": "22.13.1"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@sentry/cli",
      "@swc/core",
      "canvas",
      "esbuild",
      "puppeteer",
      "sharp"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
