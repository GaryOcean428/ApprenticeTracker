{
  "name": "crm7",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.8.2",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "node scripts/check-env.mjs && next dev -p 5145",
    "dev:local": "chmod +x scripts/start-dev.sh && scripts/start-dev.sh",
    "build": "node scripts/check-env.mjs && NODE_NO_WARNINGS=1 NODE_OPTIONS='--no-warnings' next build",
    "start": "node scripts/check-env.mjs && next start -p 5145",
    "lint": "next lint",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "types": "supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > lib/types/database.ts",
    "types:watch": "nodemon --watch supabase/migrations -e sql -x 'yarn types'",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "types:sync": "supabase gen types typescript --local > lib/database.types.ts",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "validate": "yarn lint && yarn test && yarn build",
    "prepare": "husky"
  },
